{
  "hosting": {
    "public": "dist",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/watchfaces-privacy-policy",
        "destination": "/watchfaces-privacy-policy.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
